    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/utils": "^5.11.9",
    "@react-icons/all-files": "^4.1.0",
    "@react-oauth/google": "^0.12.1",
    "@react-pdf-viewer/core": "^3.12.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.2.3",
    "crypto-browserify": "^3.12.0",
    "firebase": "^10.7.1",
    "graphql": "^16.8.0",
    "nodemailer": "^6.9.13",
    "pako": "^2.1.0",
    "playwright": "^1.44.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.1",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-dom-confetti": "^0.2.0",
    "react-icons": "^4.9.0",
    "react-paginate": "^8.2.0",
    "react-pdf": "^7.5.1",
    "react-redux": "^8.1.3",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-toolkit": "^1.1.2",
    "styled-components": "^5.3.9",
    "swiper": "^11.0.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject",
    "generate-public-files": "node src/createPublicFiles.js",
    "cypress": "cypress open",
    "cypress:headless": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../frontend/src/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/../frontend/src/mocks/styleMock.js",
      "^axios$": "axios/dist/node/axios.cjs",
      "swiper/react": "<rootDir>/../frontend/src/mocks/swiperReact.js",
      "swiper/modules": "<rootDir>/../frontend/src/mocks/swiper.js",
      "swiper/css": "<rootDir>/../frontend/src/mocks/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.1",
    "@cfaester/enzyme-adapter-react-18": "^0.8.0",
    "@cypress/react18": "^2.0.1",
    "@cypress/webpack-dev-server": "^3.10.0",
    "autoprefixer": "^10.4.16",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.11.0",
    "cypress-file-upload": "^5.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.31",
    "react-router-dom": "^6.8.1",
    "redux-mock-store": "^1.5.4",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.3.5"
  }
}
